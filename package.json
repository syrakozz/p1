    "magic:ios": "yarn copyAssets --wl=magic --p=ios; ENVFILE=.env.magic react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "format": "prettier --plugin-search-dir=. --cache --write .",
    "format:check": "prettier --plugin-search-dir=. --cache --check .",
    "updates": "npx npm-check-updates -i -p yarn",
