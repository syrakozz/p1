  "scripts": {
    "build:2xl:android": "yarn copyAssets --wl=2xl --p=android; yarn androidConfig --bundleid=com.d1srupt1ve.my2xl; ENVFILE=.env.2xl npx react-native build-android --mode=release",
    "build:magic:android": "yarn copyAssets --wl=magic --p=android; yarn androidConfig --bundleid=com.d1srupt1ve.magical; ENVFILE=.env.magic npx react-native build-android --mode=release",
    "2xl:android": "yarn copyAssets --wl=2xl --p=android; yarn androidConfig --bundleid=com.d1srupt1ve.my2xl; adb reverse tcp:8888 tcp:8888; adb reverse tcp:9090 tcp:9090; ENVFILE=.env.2xl react-native run-android",
    "2xl:ios": "yarn copyAssets --wl=2xl --p=ios; ENVFILE=.env.2xl react-native run-ios",
